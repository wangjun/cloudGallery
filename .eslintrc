{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
      "no-underscore-dangle": [
        0
      ],
      "new-cap": [
        0
      ]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true
    },
  "globals": {
    "loadImage": true,
    "validator": true,
    "Uploader": true
  }
}